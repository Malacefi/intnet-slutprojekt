<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="intnet17.projektet">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.StartpageView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.PlaylistListView" />
        <activity android:name=".view.PlaylistView" />
        <activity android:name=".view.CreateAccountView" />
        <activity
            android:name=".view.CreatePlaylistPopup"
            android:theme="@style/PopupTheme"/>
        <activity
            android:name=".view.AddSongPopup"
            android:theme="@style/PopupTheme"></activity>
    </application>

</manifest>